<section id="hero-1426">
  <div class="cs-container">
    <div class="cs-content"
         data-0="transform:translateY(0px); opacity:1;"
         data-500="transform:translateY(-50px); opacity:0;">
      <h1 class="cs-title">Fresh Leafy Greens for Wholesale</h1>
      <p class="cs-text">
        Consistent supply, certified quality, fast delivery.
      </p>
      <div class="cs-button-group">
        <a href="/AmazingSalads/products" class="cs-button-solid cs-button1">Browse Products</a>
        <a href="https://wa.me/447463595566" class="cs-button-solid cs-button2">Request Wholesale Pricing</a>
      </div>
    </div>
  </div>

  <picture class="cs-background"
           data-0="transform:translateY(0px) scale(1); opacity:1;"
           data-500="transform:translateY(100px) scale(1.05); opacity:0.5;">
    <source media="(max-width: 600px)" srcset="/AmazingSalads/images/hero.png">
    <source media="(min-width: 601px)" srcset="/AmazingSalads/images/hero.png">
    <img loading="lazy" decoding="async" src="/AmazingSalads/images/hero.png" alt="field" width="1920" height="1200" aria-hidden="true">
  </picture>
</section>

<style>
#hero-1426 {
  text-align: center;
  padding: clamp(12.5rem, 25.95vw, 18.75em) 1rem;
  overflow: hidden;
  position: relative;
}

#hero-1426 .cs-background {
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -2;
}

#hero-1426 .cs-background:before {
  content: "";
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.48;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  pointer-events: none;
}

#hero-1426 .cs-background img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform 0.1s linear, opacity 0.1s linear;
}

#hero-1426 .cs-container {
  width: 100%;
  max-width: 80rem;
  margin: auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  gap: 3rem;
}

#hero-1426 .cs-content {
  width: 100%;
  max-width: 39.375rem;
  position: relative;
  z-index: 2;
  transition: transform 0.1s linear, opacity 0.1s linear;
}

#hero-1426 .cs-title {
  font-size: clamp(2.4375rem, 6.4vw, 3.8125rem);
  font-weight: 700;
  line-height: 1.2em;
  color: #fff;
}

#hero-1426 .cs-text {
  font-size: clamp(1rem, 1.95vw, 1.25rem);
  line-height: 1.5em;
  color: #fff;
  margin-bottom: 2rem;
}

#hero-1426 .cs-button-group {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  gap: 0.5rem;
}

#hero-1426 .cs-button-solid {
  font-size: 1rem;
  line-height: clamp(2.875rem, 5.5vw, 3.5rem);
  text-decoration: none;
  font-weight: 700;
  color: #1a1a1a;
  min-width: 12.5rem;
  padding: 0 1.5rem;
  border-radius: 0.5rem;
  display: inline-block;
  position: relative;
  z-index: 2;
}

#hero-1426 .cs-button1 { background-color: var(--secondary); }
#hero-1426 .cs-button2 { background-color: var(--primary); }

#hero-1426 {
  position: relative;
  height: 100vh; /* full viewport height */
  overflow: hidden;
  z-index: 1;
}

.cs-background {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 120%; /* slightly taller for parallax */
  z-index: -1; /* ensure it’s behind hero content */
}


</style>

