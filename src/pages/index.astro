/* Mobile - 360px */
@media only screen and (min-width: 0rem) {
  #gallery-1508 {
    padding: var(--sectionPadding);
    overflow: hidden;
  }
  #gallery-1508 .cs-container {
    width: 100%;
    /* changes to 1280px at tablet */
    max-width: 34.375rem;
    margin: auto;
    display: grid;
    grid-auto-flow: row;
    /* 16px - 20px */
