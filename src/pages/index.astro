<section id="gallery-1508">
    <div class="cs-container">
        <div class="cs-content">
            <span class="cs-topper">Products</span>
            <h2 class="cs-title">Explore Our Amazing Products</h2>
            <p class="cs-text">
                Take a closer look at the delicious, garden-fresh greens and herbs we grow.
            </p>
            <a href="/AmazingSalads/products" class="cs-button-solid">View All</a>
        </div>
        <!--Picture 1-->
        {galleryItems.map(product => (
          <div class="cs-picture-group fade-slide">
              <picture class="cs-image">
                  <source media="(max-width: 600px)"
                          srcset={product.image}>
                  <source media="(min-width: 601px)"
                          srcset={product.image}>
                  <img loading="lazy" decoding="async"
                          src={product.image}
                          height="400" height="400" alt="day care">
              </picture>
            
              <div class="cs-info">
                  <h3 class="cs-h3">{product.name}</h3>
                  <p class="cs-item-text">{product.description}</p>
              </div>
          </div>
        ))}
        <img class="cs-graphic cs-graphic-one"
                src="/AmazingSalads/images/svg-waves.svg"
                alt="graphic" loading="lazy" decoding="async" height="134" width="197">
        <img class="cs-graphic cs-graphic-two"
                src="https://csimg.nyc3.cdn.digitaloceanspaces.com/Images/Graphics/day-care-team-graphic-three.svg"
                alt="graphic" loading="lazy" decoding="async" height="191" width="138">
    </div>
</section>



